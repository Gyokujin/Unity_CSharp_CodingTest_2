using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Test113 : MonoBehaviour
{
    // 문자열 myString과 pat이 주어집니다. myString에서 pat이 등장하는 횟수를 return 하는 solution 함수를 완성해 주세요.

    void Start()
    {
        Solution("banana", "ana");
        Solution("aaaa", "aa");
    }

    int Solution(string myString, string pat)
    {
        string[] words = myString.Split(pat);
        int correct = words.Length;
        Debug.Log(correct);
        return correct;
    }
}
