using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Test143 : MonoBehaviour
{
    // 정수 리스트 num_list와 찾으려는 정수 n이 주어질 때, num_list안에 n이 있으면 1을 없으면 0을 return하도록 solution 함수를 완성해주세요.

    void Start()
    {
        Solution(new List<int> { 1, 2, 3, 4, 5 }, 3);
        Solution(new List<int> { 15, 98, 23, 2, 15 }, 20);
    }

    int Solution(List<int> num_list, int n)
    {
        int correct = num_list.Contains(n) ? 1 : 0;
        
        Debug.Log(correct);
        return correct;
    }
}
